@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Vidly.Models.Customer;

<div>
    
    
        <h2>@Model.Name </h2>
        <ul>
           
        @if(Model.BirthDate != null)
        {
            DateTime temp = @Model.BirthDate ?? DateTime.MinValue;
            <li><text>Birthdate : @temp.ToString("dd/MM/yyyy") </text></li>
        }

            <li>Subscription : @Model.MembershipType.Name </</li>
        </ul>
    
</div>
